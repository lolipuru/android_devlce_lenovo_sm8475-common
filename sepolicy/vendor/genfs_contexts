genfscon sysfs /devices/platform/soc/a98000.i2c/i2c-7/7-005a/leds/vibrator_r/activate                 u:object_r:sysfs_vibrator:s0
genfscon sysfs /class/leds/vibrator/activate                                                                                                                                         u:object_r:sysfs_leds:s0
genfscon sysfs /class/leds/vibrator/state                                                                                                                                            u:object_r:sysfs_leds:s0
genfscon sysfs /class/leds/vibrator/duration                                                                                                                                         u:object_r:sysfs_leds:s0

